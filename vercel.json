{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false
}